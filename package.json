{
  "name": "react-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/**.test.ts src/tests/random.ts  --exit",
    "test:pact": "NODE_ENV=pact mocha -r ts-node/register src/pact-verification/**.pact.ts --exit",
    "test:cy": "cypress run",
    "test:e2e": "cd client && npm run build && cd ..  && start-server-and-test start http://localhost:5000 test:cy",
    "test:ci:e2e": "cd client && npm install && CI=false npm run build && cd ..  && start-server-and-test start http://localhost:5000 test:cy",
    "dev": "nodemon -x ts-node ./server.ts",
    "start": "ts-node ./server.ts",
    "storybook": "cd client && npm ci && npm run build-storybook && npm run chromatic",
    "heroku-postbuild": "cd client && npm ci --also=dev && npm run build"
  },
  "author": "",
  "license": "ISC",
